{
    "servers":[
        {
            "url":"https://{accountName}.{environment}.com.br/api",
            "description":"VTEX server url"
        }
    ],
    "paths":{
        "/sessions":{
            "post":{
                "operationId":"CreateNewSession",
                "parameters":[
                    {
                        "name":"accountName",
                        "in":"path",
                        "required":true,
                        "schema":{
                            "type":"string",
                            "default":"apiexamples"
                        }
                    },
                    {
                        "name":"environment",
                        "in":"path",
                        "required":true,
                        "schema":{
                            "type":"string",
                            "default":"vtexcommercestable"
                        }
                    }
                ],
                "deprecated":false
            },
            "get":{
                "operationId":"GetSession",
                "parameters":[
                    {
                        "name":"accountName",
                        "in":"path",
                        "required":true,
                        "schema":{
                            "type":"string",
                            "default":"apiexamples"
                        }
                    },
                    {
                        "name":"environment",
                        "in":"path",
                        "required":true,
                        "schema":{
                            "type":"string",
                            "default":"vtexcommercestable"
                        }
                    },
                    {
                        "name":"items",
                        "in":"query",
                        "required":true,
                        "style":"form",
                        "explode":true,
                        "schema":{
                            "type":"string",
                            "example":"namespace1.key1,namespace2.key2"
                        }
                    }
                ],
                "deprecated":false
            },
            "patch":{
                "operationId":"EditSession",
                "parameters":[
                    {
                        "name":"accountName",
                        "in":"path",
                        "required":true,
                        "schema":{
                            "type":"string",
                            "default":"apiexamples"
                        }
                    },
                    {
                        "name":"environment",
                        "in":"path",
                        "required":true,
                        "schema":{
                            "type":"string",
                            "default":"vtexcommercestable"
                        }
                    }
                ],
                "requestBody":{
                    "description":"",
                    "content":{
                        "application/json":{
                            "schema":{
                                "$ref":"#/components/schemas/EditsessionRequest"
                            },
                            "example":{
                                "public":{
                                    "newValue":{
                                        "value":"patched"
                                    }
                                }
                            }
                        }
                    },
                    "required":true
                },
                "deprecated":false
            }
        },
        "/segments":{
            "get":{
                "operationId":"GetSegment",
                "parameters":[
                    {
                        "name":"accountName",
                        "in":"path",
                        "required":true,
                        "schema":{
                            "type":"string",
                            "default":"apiexamples"
                        }
                    },
                    {
                        "name":"environment",
                        "in":"path",
                        "required":true,
                        "schema":{
                            "type":"string",
                            "default":"vtexcommercestable"
                        }
                    }
                ],
                "deprecated":false
            }
        }
    },
    "components":{
        "securitySchemes":{
            "appKey":{
                "type":"apiKey",
                "in":"header",
                "name":"X-VTEX-API-AppKey"
            },
            "appToken":{
                "type":"apiKey",
                "in":"header",
                "name":"X-VTEX-API-AppToken"
            }
        }
    }
}