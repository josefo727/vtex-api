{
    "servers":[
        {
            "url":"https://{accountName}.{environment}.com.br"
        }
    ],
    "paths":{
        "/api/mail-service/pvt/providers/{EmailProvider}/dkim":{
            "post":{
                "operationId":"GenerateDKIMKeys",
                "parameters":[
                    {
                        "name":"accountName",
                        "in":"path",
                        "required":true,
                        "schema":{
                            "type":"string",
                            "default":"apiexamples"
                        }
                    },
                    {
                        "name":"environment",
                        "in":"path",
                        "required":true,
                        "schema":{
                            "type":"string",
                            "default":"vtexcommercestable"
                        }
                    },
                    {
                        "name":"EmailProvider",
                        "in":"path",
                        "required":true,
                        "schema":{
                            "type":"string",
                            "default":"help@valdie.co"
                        }
                    }
                ]
            }
        }
    },
    "components":{
        "securitySchemes":{
            "appKey":{
                "type":"apiKey",
                "in":"header",
                "name":"X-VTEX-API-AppKey"
            },
            "appToken":{
                "type":"apiKey",
                "in":"header",
                "name":"X-VTEX-API-AppToken"
            }
        }
    }
}